{
  "id": "bc-template-keystone",
  "description": "Centralized authentication and authorization service for OpenStack",
  "attributes": {
    "keystone": {
      "debug": false,
      "frontend": "uwsgi",
      "verbose": true,
      "gitrepo": "http://github.com/openstack/keystone.git",
      "git_instance": "",
      "git_refspec": "stable/havana",
      "use_gitbarclamp": true,
      "use_pip_cache": true,
      "use_gitrepo": false,
      "use_virtualenv": true,
      "pfs_deps": [
          "pip://setuptools-git",
          "pip://distribute>=0.6.28",
          "pip://MySQL-python"
      ],
      "use_syslog": false,
      "database_instance": "none",
      "db": {
        "database": "keystone",
        "user": "keystone"
      },
      "sql": {
        "idle_timeout": 30
      },
      "ssl": {
        "certfile": "/etc/keystone/ssl/certs/signing_cert.pem",
        "keyfile": "/etc/keystone/ssl/private/signing_key.pem",
        "generate_certs": false,
        "insecure": false,
        "cert_required": false,
        "ca_certs": "/etc/keystone/ssl/certs/ca.pem"
      },
      "api": {
        "protocol": "http",
        "service_port": 5000,
        "api_host": "0.0.0.0",
        "admin_port": 35357,
        "admin_host": "0.0.0.0"
      },
      "admin": {
        "tenant": "admin",
        "username": "admin",
        "password": "crowbar"
      },
      "service": {
        "tenant": "service",
        "token": "999888777666"
      },
      "default": {
        "tenant": "openstack",
        "username": "crowbar",
        "password": "crowbar"
      },
      "identity": {
        "driver": "keystone.identity.backends.sql.Identity"
      },
      "assignment": {
        "driver": "keystone.assignment.backends.sql.Assignment"
      },
      "signing": {
        "token_format": "PKI",
        "certfile": "/etc/keystone/ssl/certs/signing_cert.pem",
        "keyfile": "/etc/keystone/ssl/private/signing_key.pem",
        "ca_certs": "/etc/keystone/ssl/certs/ca.pem"
      },
      "ldap" : {
        "url": "ldap://localhost",
        "user": "dc=Manager,dc=example,dc=com",
        "password": "",
        "suffix": "cn=example,cn=com",
        "use_dumb_member": false,
        "allow_subtree_delete": false,
        "dumb_member": "cn=dumb,dc=example,dc=com",
        "page_size": 0,
        "alias_dereferencing": "default",
        "query_scope": "one",
        "user_tree_dn": "",
        "user_filter": "",
        "user_objectclass": "inetOrgPerson",
        "user_id_attribute": "cn",
        "user_name_attribute": "sn",
        "user_mail_attribute": "email",
        "user_pass_attribute": "userPassword",
        "user_enabled_attribute": "enabled",
        "user_enabled_mask": 0,
        "user_enabled_default": true,
        "user_attribute_ignore": "tenant_id,tenants",
        "user_default_project_id_attribute": "",
        "user_allow_create": true,
        "user_allow_update": true,
        "user_allow_delete": true,
        "user_enabled_emulation": false,
        "user_enabled_emulation_dn": "",
        "tenant_tree_dn": "",
        "tenant_filter": "",
        "tenant_objectclass": "groupOfNames",
        "tenant_domain_id_attribute": "businessCategory",
        "tenant_id_attribute": "cn",
        "tenant_member_attribute": "member",
        "tenant_name_attribute": "ou",
        "tenant_desc_attribute": "description",
        "tenant_enabled_attribute": "enabled",
        "tenant_attribute_ignore": "",
        "tenant_allow_create": true,
        "tenant_allow_update": true,
        "tenant_allow_delete": true,
        "tenant_enabled_emulation": false,
        "tenant_enabled_emulation_dn": "",
        "role_tree_dn": "",
        "role_filter": "",
        "role_objectclass": "organizationalRole",
        "role_id_attribute": "cn",
        "role_name_attribute": "ou",
        "role_member_attribute": "roleOccupant",
        "role_attribute_ignore": "",
        "role_allow_create": true,
        "role_allow_update": true,
        "role_allow_delete": true,
        "group_tree_dn": "",
        "group_filter": "",
        "group_objectclass": "groupOfNames",
        "group_id_attribute": "cn",
        "group_name_attribute": "ou",
        "group_member_attribute": "member",
        "group_attribute_ignore": "",
        "group_allow_create": true,
        "group_allow_update": true,
        "group_allow_delete": true
      }
    }
  },
  "deployment": {
    "keystone": {
      "crowbar-revision": 0,
      "schema-revision": 3,
      "element_states": {
        "keystone-server": [ "readying", "ready", "applying" ]
      },
      "elements": {},
      "element_order": [
         ["keystone-server" ]
      ],
      "config": {
        "environment": "keystone-config-base",
        "mode": "full",
        "transitions": false,
        "transition_list": []
      }
    }
  }
}
